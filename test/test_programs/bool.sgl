var tBool = true;
var fBool = false;

println("true == true: " + (true == true).toString()); // output true == true: true
println("tBool == fBool: " + (tBool == fBool).toString()); // output tBool == fBool: false
println("tBool && fBool: " + (tBool && fBool).toString()); // output tBool && fBool: false
println("tBool: " + tBool.toString()); // output tBool: true
println("tbool asNum: " + tBool.asNum().toString()); // output tbool asNum: 1.000000
println("tBool type: " + tBool.type); // output tBool type: bool
println("tBool isEmpty: " + "Senegal".isEmpty().toString()); // output tBool isEmpty: false
